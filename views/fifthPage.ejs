<html lang="en">

<head>
    <meta charset="utf-8">
    <title>MPN</title>
    <%- include('./layout/head') -%>
        <link rel="stylesheet" type="text/css" href="/assets/css/fifth.css">
</head>

<body>
    <div id="app">
        <div class="frame">
            <div class="div">
                <div class="overlap">
                    <div class="fluid-container">
                        <div class="row">
                            <div class="col-8">
                                <div class="overlap-group">
                                    <div class="OBJECTS">
                                        <div class="overlap-2"><img class="img" alt="Element"
                                                src="/assets/img/--29@2x.png">
                                            <img class="element-2" alt="Element" src="/assets/img/---84.svg">
                                            <div class="element-wrapper">
                                                <div class="div-wrapper">
                                                    <div class="text-wrapper">MPN</div>
                                                </div>
                                            </div>
                                            <img class="element-3" alt="Element" src="/assets/img/---9.svg">
                                            <div class="element-4">
                                                <div class="text-wrapper-2">Sample &amp; reagents loading</div>
                                            </div>
                                            <img class="element-6" alt="Element" src="/assets/img/--37@2x.png">
                                            <div class="overlap-group-wrapper">
                                                <div class="overlap-group-2">
                                                    <div id="statusNumber1" class="text-wrapper-3">01</div>
                                                </div>
                                            </div>
                                            <img class="element-19" alt="Element" src="/assets/img/---91.svg">
                                            <img class="element-7" alt="Element" src="/assets/img/---87.svg">
                                            <div class="element-8">
                                                <div class="text-wrapper-2">1st PCR</div>
                                            </div>
                                            <img class="element-11" alt="Element" src="/assets/img/--48@2x.png">
                                            <img class="element-12" alt="Element" src="/assets/img/---12.svg">
                                            <div class="element-13">
                                                <div class="text-wrapper-2">1st PCR products purification</div>
                                            </div>
                                            <img class="element-18" alt="Element" src="/assets/img/--58@2x.png">

                                            <img class="element-50" alt="Element" src="/assets/img/---12.svg">
                                            <div class="element-51">
                                                <div class="text-wrapper-2">2nd PCR</div>
                                            </div>
                                            <img class="element-52" alt="Element" src="/assets/img/--58@2x.png">


                                            <div class="element-20">
                                                <div class="text-wrapper-2">2nd PCR products purification</div>
                                            </div>
                                            <img class="element-23" alt="Element" src="/assets/img/--68@2x.png">
                                            <img class="element-24" alt="Element" src="/assets/img/---87.svg">
                                            <div class="element-25">
                                                <div id="pcText" class="text-wrapper-2">Program Complete</div>
                                            </div>
                                            <img class="element-28" alt="Element" src="/assets/img/--78@2x.png">
                                            <img class="element-29" alt="Element" src="/assets/img/---95.svg">
                                            <div id="statusNumber2" class="text-wrapper-5">02</div>
                                            <div id="statusNumber3" class="text-wrapper-6">03</div>
                                            <div id="statusNumber4" class="text-wrapper-7">04</div>
                                            <div id="statusNumber5" class="text-wrapper-8">05</div>
                                            <div id="statusNumber6" class="text-wrapper-44">06</div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-4 text-center">
                                <div class="row my-5">
                                    <div class="element">Remaining Time: <span id="timer">00:00:00</span>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-6 py-5">
                                        <button id="pause" type="button"
                                            class="chooseBtn nextPage btn-primary border-5">
                                            <i class="fa-solid fa-pause"></i>
                                        </button>
                                    </div>
                                    <div class="col-6 py-5">
                                        <button id="start" type="button"
                                            class="chooseBtn nextPage btn-primary border-5">
                                            <i class="fa-solid fa-caret-right"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="row">
                                    <button type="button" class="btn btn-warning btn-confirm">End Program</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <img class="element-43" alt="Element" src="/assets/img/---1-1@2x.png">
            </div>
        </div>
    </div>

    <div class="modal fade" id="EOPModal" tabindex="-1" aria-labelledby="EOPModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-fullscreen">
            <div class="modal-content">
                <div class="modal-body">
                    <div class="container text-center">
                        <div class="row modalFirstRow">
                            <p>End Program?</p>
                        </div>
                        <div class="row justify-content-md-center my-5">
                            <div class="col-3">
                                <button type="button" class="btn chooseBtn" data-bs-dismiss="modal">&Chi;</button>
                            </div>
                            <div class="col-3">
                                <button id="confirmPOE" type="button" class="btn chooseBtn">&radic;</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <%- include('./layout/js') -%>
        <script src="/assets/js/fifthPage.js"></script>
</body>

</html>