<!DOCTYPE html>
<html>

<head>
    <title>
        MPN
    </title>
    <%- include('./layout/head') -%>
    <link rel='stylesheet' href='/assets/css/third.css' />
</head>


<body>
    <div class="container text-center secondpage-layout">
        <div class="row mt-5 mb-5">
            <h1>Check list of Sample, Reagents, and Consumables</h1>
        </div>
        <div class="row checkSOP">
            <div class="col border border-2">
                <input id="ck_trx1" class="bigcheckbox" type="checkbox">
                <h5>Tip rack X1</h5>
            </div>
            <div class="col  border border-2">
                <div class="row">
                    <div class="col">
                        <input id="ck_reagents" class="smallcheckbox" type="checkbox">
                        <h5>Reagents</h5>
                    </div>
                </div>
                <div class="row">
                    <div class="col">
                        <input id="ck_sample" class="smallcheckbox" type="checkbox">
                        <h5>Sample</h5>
                    </div>
                </div>
                <div class="row">
                    <div class="col">
                        <input id="ck_ipm" class="smallcheckbox" type="checkbox">
                        <h5>Index PCR mix</h5>
                    </div>

                </div>
            </div>
            <div class="col border border-2">
                <input id="ck_96wpp" class="bigcheckbox" type="checkbox">
                <h5>
                    96 well PCR plate
                </h5>
            </div>
            <div class="col"></div>
        </div>
        <div class="row checkSOP">
            <div class="col border border-2">
                <input id="ck_trx3" class="bigcheckbox" type="checkbox">
                <h5>
                    Tip rack X3
                </h5>
            </div>
            <div class="col border border-2 boder-dashed"></div>
            <div class="col border border-2 boder-dashed"></div>
            <div class="col"></div>
        </div>
        <div class="row checkSOP">
            <div class="col border border-2 boder-dashed"></div>
            <div class="col border border-2">
                <input id="ck_12wpwr" class="bigcheckbox" type="checkbox">
                <h5>12 well plate
                    with reagent</h5>
            </div>
            <div class="col border border-2">
                <input id="ck_dwp" class="bigcheckbox" type="checkbox">
                <h5>
                    Deep well
                    plate
                </h5>
            </div>
            <div class="col">
                <div class="done border border-2">
                    <h5>Ashcan</h5>
                </div>
                <input id="ck_ashcan" class="bigcheckbox" type="checkbox">
            </div>
        </div>
        <div class="row checkSOP">
            <div class="col border border-2 boder-dashed"></div>
            <div class="col"></div>
            <div class="col border border-2">
                <input id="ck_erc" class="bigcheckbox" type="checkbox">
                <h5>
                    Empty racks
                    Cleaned
                </h5>
            </div>
            <div class="col"></div>
        </div>
        <div class="row mt-5 justify-content-between">
            <div class="col-2">
                <a href="/second"><button type="button" class="prevPage btn-primary border-5">prevPage</button></a>
            </div>
            <div class="col-2">
                <button type="button" class="nextPage btn-primary border-5">nextPage</button>
            </div>

        </div>
    </div>
    <%- include('./layout/js') -%>
    <script src="/assets/js/third.js"></script>
</body>

</html>